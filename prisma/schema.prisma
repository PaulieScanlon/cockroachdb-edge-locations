generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}


model locations {
  id            BigInt          @id @default(autoincrement())
  date          DateTime        @db.Date  
  city          String
  lat           Decimal
  lng           Decimal
  runtime       String?
}